class Solution {
    public boolean isAnagram(String s, String t) {

        if(s.length() != t.length())
		{
			return false;
		}
		HashMap<Character,Integer> mum = new HashMap<>();
		char[] ss =s.toCharArray();
		char[] tt =t.toCharArray();

		for(char c : ss)
		{
			if(mum.containsKey(c))
			{
				mum.put(c,mum.get(c)+1);
			}
			else {
				mum.put(c,1);
			}
		}

		for(char c :tt)
		{
			if(mum.containsKey(c))
			{
				int freq = mum.get(c) -1;
				if(freq ==0)
				{
					mum.remove(c);
				}
				else {
					mum.put(c,freq);
				}
			}
			else
			{
				return false;
			}
		}
		return true;
    }
}
